#include "main.h"
/**
 * print_alphabet_x10 - Function print_alphabet_x10 in lowercase
 */
void print_alphabet_x10(void)
int main(void)
{
int j;
for (j = 0; j < 10; j++)
{
print_alphabet_x10();
putchar('\n');
}
return (0);
}
void print_alphabet_x10(void)
{
char n[26];
int i;
for (i = 0; i < 26; i++)
{
n[i] = 'a' + i;
putchar(n[i]);
}
}
